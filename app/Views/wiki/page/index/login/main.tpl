{include file="radishadmin/widgets/header/header.tpl"}
<link rel="stylesheet" type="text/css" href="//img.oranzh.cc/wiki/static/page/index/login/login_795dddd.css" />
<div id="logo">
  <img src="//img.oranzh.cc/wiki/static/images/login-logo_f2453d4.png" alt="logo" />
</div>
<div id="loginbox">
  <form id="loginform" class="form-vertical">
    <div class="control-group normal_text">
      <h3>小萝卜后台管理系统</h3></div>
    <div class="control-group">
      <div class="controls">
        <div class="main_input_box">
          <span class="add-on"><i class="fa fa-user"></i></span>
          <input id='name' name='tel' type="text" placeholder="用户名" autocomplete="off" maxlength="11" />
        </div>
      </div>
    </div>
    <div class="control-group">
      <div class="controls">
        <div class="main_input_box">
          <span class="add-on"><i class="fa fa-lock"></i></span>
          <input id='pwd' name='pw' type="password" placeholder="密码" autocomplete="off" />
        </div>
      </div>
    </div>
    <div class="form-actions">
      <span class="pull-right"><button type="submit" id='login-btn' class="btn btn-primary">登录</button></span>
    </div>
  </form>
</div>

  
  <script type="text/javascript" src="//img.oranzh.cc/wiki/static/page/index/login/login_13eacaf.js"></script>

  {include file="radishadmin/widgets/footer/footer.tpl"}